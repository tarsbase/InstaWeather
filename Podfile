platform :ios, '10.0'

target 'Rain Check' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Rain Check
  pod 'SwiftyJSON'
  pod 'Alamofire'
  pod 'SVProgressHUD'

  target 'Rain CheckTests' do
    inherit! :search_paths
    # Pods for testing
pod 'SwiftyJSON'
pod 'Alamofire'
pod 'SVProgressHUD'
  end

  target 'Rain CheckUITests' do
    inherit! :search_paths
    # Pods for testing
pod 'SwiftyJSON'
pod 'Alamofire'
pod 'SVProgressHUD'
  end

end
